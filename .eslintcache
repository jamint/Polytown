    />\n                <spotLight\n                    intensity={1}\n                    position={[-5, 5, 5]}\n                    castShadow\n                    penumbra={0.5}\n                    shadowBias={-0.0004}\n                    angle={Math.PI / 4}\n                    shadow-mapSize-height={1024}\n                    shadow-mapSize-width={1024}\n                />\n                <pointLight\n                    intensity={1.5}\n                    position={[1, 5, 1]}\n                    castShadow\n                    shadowBias={-0.0004}\n                    shadow-mapSize-height={1024}\n                    shadow-mapSize-width={1024}\n                />\n                <Suspense fallback={null}>\n                    <Schnitzel />\n                </Suspense>\n                {/* <OrbitControls enableZoom={false} /> */}\n                <OrbitControls />\n            </Canvas>\n        </div >\n    );\n}\n\nexport default Animation;","/Users/jad.mintun/Documents/Git Projects/Polytown/src/components/Main.js",[],"/Users/jad.mintun/Documents/Git Projects/Polytown/src/pages/About.js",[],"/Users/jad.mintun/Documents/Git Projects/Polytown/src/pages/Home.js",[],"/Users/jad.mintun/Documents/Git Projects/Polytown/src/pages/Shop.js",[],{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","severity":1,"message":"56","line":6,"column":3,"nodeType":"57","messageId":"58","endLine":6,"endColumn":7},{"ruleId":"55","severity":1,"message":"59","line":7,"column":3,"nodeType":"57","messageId":"58","endLine":7,"endColumn":11},{"ruleId":"55","severity":1,"message":"60","line":8,"column":3,"nodeType":"57","messageId":"58","endLine":8,"endColumn":17},{"ruleId":"55","severity":1,"message":"61","line":9,"column":3,"nodeType":"57","messageId":"58","endLine":9,"endColumn":17},{"ruleId":"55","severity":1,"message":"62","line":3,"column":39,"nodeType":"57","messageId":"58","endLine":3,"endColumn":48},{"ruleId":"55","severity":1,"message":"63","